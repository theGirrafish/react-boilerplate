{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for a React application",
  "main": "index.jsx",
  "engines": {
    "npm": "6.4.1",
    "node": "11.1.0"
  },
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.dev.config.js",
    "prod": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.prod.config.js",
    "build": "./node_modules/.bin/webpack --config ./webpack/webpack.prod.config.js",
    "start": "node server.jsx",
    "lint": "./node_modules/.bin/eslint ./src/**/*.js* ./webpack/**/*.js*",
    "test": "./node_modules/.bin/jest",
    "test:watch": "./node_modules/.bin/jest --watchAll"
  },
  "heroku-run-build-script": true,
  "author": "<Your Name>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<yourgithubusername>/<yourrepo>.git"
  },
  "bugs": {
    "url": "https://github.com/<yourgithubusername>/<yourrepo>/issues"
  },
  "homepage": "https://github.com/<yourgithubusername>/<yourrepo>#readme",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup.jsx"
    ],
    "moduleNameMapper": {
      ".+\\.(png|jpg|jpeg|gif|svg|ico|eot|ttf|woff|woff2)$": "<rootDir>/test/mocks/fileMock.jsx"
    }
  },
  "dependencies": {
    "@types/react": "^16.8.5",
    "axios": "^0.18.0",
    "lodash": "^4.17.10",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^1.0.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.10",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.14.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.0.6",
    "node-sass": "^4.11.0",
    "react-test-renderer": "^16.8.3",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.18.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
